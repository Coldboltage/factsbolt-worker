  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev:prod:test": "cross-env NODE_ENV=production nest start --watch",
    "start:dev:local": "npm run build && cross-env NODE_ENV=local node --max-old-space-size=16384 -r ts-node/register -r tsconfig-paths/register dist/main.js",
    "start:dev:local:scrapper": "cross-env NODE_ENV=local QUEUE_ENV=scrapper-queue nest start --watch",
    "start:dev:prod": "npm run build && cross-env NODE_ENV=production node --max-old-space-size=16384 -r ts-node/register -r tsconfig-paths/register dist/main.js",
